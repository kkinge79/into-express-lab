<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>greatestMovies</title>
</head>
<body>
  <h1>Greatest Movies</h1>
  <% if(error) { %>
    <h2><%= error %></h2>
    <% } %>
    <ul>
      <% greatestMovies.forEach(function(greatestMovie) { %>
        <li>
          <%= greatestMovie.text %>
          -
          <%= greatestMovie.year %>
          -
          <%= greatestMovie.ratings %>
        </li>
      <% }) %>
    </ul>
</body>
</html>